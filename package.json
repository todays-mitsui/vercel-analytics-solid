{
  "name": "vercel-analytics-solid",
  "description": "A lightweight SolidStart component that seamlessly integrates Vercel Analytics into your SolidJS applications.",
  "version": "0.1.1",
  "author": "Mitsui <todays.mitsui@gmail.com>",
  "bugs": {
    "url": "https://github.com/todays-mitsui/vercel-analytics-solid/issues"
  },
  "dependencies": {
    "@vercel/analytics": "^1.4.1",
    "vite-plugin-solid": "^2.11.1"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^26.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "exports": {
    ".": "./src/solid.tsx",
    "./solidstart": "./src/solidstart.tsx"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/todays-mitsui/vercel-analytics-solid#readme",
  "keywords": [
    "analytics",
    "component",
    "solidjs",
    "solidstart",
    "vercel",
    "web-analytics"
  ],
  "license": "MIT",
  "module": "./src/solid.tsx",
  "peerDependencies": {
    "@solidjs/router": "^0.15.1",
    "solid-js": "^1.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/todays-mitsui/vercel-analytics-solid.git"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:type",
    "build:js": "tsup --format=esm src/solid.tsx src/solidstart.tsx",
    "build:type": "npx tsc --declaration --outDir ./dist/ --emitDeclarationOnly ./src/{solid,solidstart}.tsx",
    "check": "npx @biomejs/biome check --write ./src",
    "fmt": "npx @biomejs/biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "type": "module"
}
